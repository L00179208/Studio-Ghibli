		{
			"name": "LoginUser",
			"event": [
				{
					"listen": "test",
					"script": {
						"exec": [
							"var loginData = {",
							"    \"email\": pm.iterationData.get('email'),",
							"    \"password\": pm.iterationData.get('password')",
							"};",
							"",
							"// CREATE Course Send Request",
							"pm.sendRequest({",
							"    //url: 'http://localhost:8884/create?extended=true&details=all', ",
							"    url: 'http://localhost:8881/login',",
							"    method: 'POST',",
							"    header: {",
							"        'Content-Type': 'application/json',",
							"    },",
							"    body: {",
							"        mode: 'raw',",
							"        raw: JSON.stringify(loginData)",
							"    }",
							"}, function (err, res) {",
							"    if (err) {",
							"        console.error(err);",
							"        return;",
							"    }",
							"",
							"    //Status Code 200",
							"    pm.test(\"Validate Status code 200\", function () {",
							"        pm.expect(res).to.have.status(200);",
							"    });",
							"",
							"    // Check response time less than 300ms",
							"    pm.test(\"Validate response time below 300\", function() {",
							"        pm.expect(res.responseTime).to.be.below(300);",
							"    });",
							"",
							"    pm.test('Authentication successful', function () {",
							"        pm.expect(res).to.not.be.null;",
							"        pm.expect(res.json().message).to.equal('Authentication successful');",
							"        pm.expect(res.json()).to.have.property('token');",
							"    })",
							"",
							"    pm.test('Token format is correct', function () {",
							"        pm.expect(res.json().token).to.be.a('string');",
							"        pm.expect(res.json().token).to.match(/^eyJ[a-zA-Z0-9_-]*\\.[a-zA-Z0-9_-]*\\.[a-zA-Z0-9_-]*$/);",
							"    });",
							"",
							"    pm.test('Validate Status code 500', function () {",
							"        pm.expect(res).to.not.be.null;",
							"        pm.expect(res.status).to.be.eq(500);",
							"        pm.expect(res.json().message).to.match(/(Auth Controller)/);",
							"    })",
							"",
							"",
							"    ",
							"",
							"});",
							"",
							"",
							"",
							"",
							"",
							"",
							""
