							"        pm.expect(userData).to.have.property(\"id\");",
							"        pm.expect(userData).to.have.property(\"user_id\");",
							"        pm.expect(userData).to.have.property(\"full_name\");",
							"        pm.expect(userData).to.have.property(\"phone_no\");",
							"        pm.expect(userData).to.have.property(\"area_of_interests\");",
							"        pm.expect(userData).to.have.property(\"status\");",
							"        pm.expect(userData).to.have.property(\"updated_at\");",
							"        pm.expect(userData).to.have.property(\"created_at\");",
